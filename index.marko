<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Square Frustum</title>
  <meta name="google" content="notranslate" />
  <link rel="shortcut icon" href="assets/favicon.webp" type="image/x-icon" />
  <link rel="stylesheet" href="style.css" />
</head>
<body>
  $ const lados = ["top", "bottom", "behind", "front", "right", "left"];
  $ const sides = ["bottom", "top", "right", "left", "front", "behind"];
  $ const scenes = ["scene1", "scene2"];

  $ scenes.forEach(scene => {
    <div class={`scene ${scene}`}>
      $ lados.forEach(lado => {
        <div class={`lado lado-${lado}`}></div>
      })
    </div>
  })

  <div class="container">
    $ sides.forEach(side => {
      <div class={`side side-${side}`}></div>
    })
  </div>

  <script src="script.js"></script>
</body>
</html>
